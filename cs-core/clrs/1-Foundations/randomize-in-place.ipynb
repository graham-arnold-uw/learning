   "source": [
    "import random\n",
    "\n",
    "def randomize_in_place(A):\n",
    "    n = len(A)\n",
    "    \n",
    "    for i in range(n):\n",
    "        temp = A[i]\n",
    "        \n",
    "        dest_idx = random.randint(i, n-1)\n",
    "        A[i] = A[dest_idx]\n",
    "        A[dest_idx] = temp\n",
    "    \n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[3, 4, 5, 6, 2, 1]\n"
     ]
    }
   ],
   "source": [
    "T = [1,2,3,4,5,6]\n",
    "randomize_in_place(T)\n",
    "print(T)"
   ]
  },
